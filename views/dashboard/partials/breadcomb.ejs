<% if(page == '/dashboard') { %>
    <% if(user.admin) { %>
        <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Início</a>
    <% } else { %>
        <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Projetos</a>
    <% } %>
<% } else if(page == '/profile') { %>
<a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Perfil</a>
<% } else if(page == '/project') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Projetos - <%= project.name %></a>
<% } else if(page == '/budget') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Novo projeto</a>
<% } else if(page == '/users') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Usuários</a>
<% } else if(page == '/allprojects') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Projetos</a>
<% } else if(page == '/viewUser') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Usuário - <%= getUser.name %></a>
<% } else if(page == '/message' || page == '/message-detail') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Mensagens</a>
<% } else if(page == '/invoice') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Faturas</a>
<% } else if(page == '/support') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Suporte</a>
<% } else if(page == '/portfolio') { %>
    <a class="h4 mb-0 text-white text-uppercase d-none d-lg-inline-block">Portfólio</a>
<% } %>